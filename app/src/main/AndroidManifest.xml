<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.CampusFinal"
        tools:targetApi="31">
        <activity
            android:name="com.naeem.campusfinal.ui.home.project.ProjectActivity"
            android:exported="false" />
        <activity
            android:name="com.naeem.campusfinal.ui.home.upcomingevent.UpcomingEventActivity"
            android:parentActivityName="com.naeem.campusfinal.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.naeem.campusfinal.ui.home.project.infotech.InfoTechActivity"
            android:parentActivityName="com.naeem.campusfinal.ui.home.project.ProjectActivity"
            android:exported="false" />
        <activity
            android:name="com.naeem.campusfinal.authentication.ForgetPassActivity"
            android:exported="true" />
        <activity
            android:name="com.naeem.campusfinal.authentication.RegisterActivity"
            android:exported="true"
            />
        <activity
            android:name="com.naeem.campusfinal.authentication.LoginActivity"
            android:exported="true" />
        <activity
            android:name="com.naeem.campusfinal.authentication.SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.naeem.campusfinal.MainActivity"
            android:exported="true" />
    </application>

</manifest>